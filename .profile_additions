# Additions to stock .profile file

# Include following codeblock to .profile:
#if [ -f ~/.profile_additions ]; then
#    . ~/.profile_additions
#fi

# Project MSDL music visualizer
export PATH="$HOME/.local/projectMSDL:$PATH"

# Adding ~/scripts to path
export PATH="$HOME/scripts:$PATH"

# Adding location of flatpak binaries so that dmenu may find them
export PATH="$HOME/.local/share/flatpak/exports/bin:$PATH"

# Provide lf theming when launched via shortcut
export PATH="$HOME/.cargo/bin/:$PATH"
export LS_COLORS="$(vivid generate rose-pine-moon)"
